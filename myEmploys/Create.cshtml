            <div class="form-group">
                <label asp-for="Employs.Fname" class="control-label"></label>
                <input asp-for="Employs.Fname" class="form-control" />
                <span asp-validation-for="Employs.Fname" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Employs.Lname" class="control-label"></label>
                <input asp-for="Employs.Lname" class="form-control" />
